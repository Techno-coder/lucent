@@binary "mach-o"

@architecture "x64"
@load 1024 * 1024
module Main
	root fn main() u8
		(fibonacci(46) % 100) as u8

	fn fibonacci(n: u64) u64
		let i: u64 = 0
		let a: u64 = 0
		let b: u64 = 1
		while i < n:
			let c = a + b
			a = b
			b = c
			i += 1
		return b

	fn fibonacci(n: u32) u64
		if n <= 1: return 1
		fibonacci'(n - 1) + fibonacci'(n - 2)
